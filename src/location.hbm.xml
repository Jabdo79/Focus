<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="focus.up.Location" table="locations">
		<id name="id" column="id" type="int"><generator class="increment"></generator></id>
		<property name="name" column="name" type="string"></property>
		<property name="topics" column="topics" type="string"></property>
		<property name="address" column="studyAddress" type="string"></property>
		<property name="rating" column="rating" type="int"></property>
	</class>
</hibernate-mapping>